ansible dev -b -m copy -a "src=/home/ansible/sam.txt dest=/home/ansible/sam.txt"
ansible localhost -b -m file -a "path=/home/ansible/sam.txt state=absent"
ansible dev -b -m shell -a "cd /home/ansible/sanjana"
ansible dev -b -m file -a "path=/home/ansible/sanjana state=directory"
ansible dev -b -m file -a "path=/home/ansible/sanjana state=absent"
ansible dev -b -m file -a "path=/home/ansible/dev.txt state=touch"
ansible dev -b -m file -a "path=/home/ansible/dev.txt state=absent"
ansible dev -b -m file -a "path=/home/ansible/sam.txt state=touch mode=777"
ansible dev -b -m file -a "path=/home/ansible/sam.txt state=absent"
ansible dev -b -m yum -a "name=git state=present"
ansible dev -b -m yum -a "name=docker state=present"
ansible dev -b -m command -a "sudo docker pull httpd:latest"
ansible dev -b -m command -a "docker run -d --name webapp -p 80:80 httpd:latest"
ansible dev -b -m git -a "repo=https://github.com/Sanjana1455/perfectpet.git dest=/home/ansible/perfectpet"